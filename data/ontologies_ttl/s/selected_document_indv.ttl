@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix prop-data: <http://vivoweb.org/ontology/core/properties/individual#> .
@prefix prop:  <http://vivoweb.org/ontology/core/properties/vocabulary#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

prop-data:selected_document
        rdf:type    owl:NamedIndividual ;
        rdf:type    prop:PropertyKey ;
        rdfs:label  "selected_document" .

prop-data:selected_document.VIVO.nemo
        rdf:type         owl:NamedIndividual ;
        rdf:type         prop:PropertyKey ;
        rdfs:label       "selected_document.VIVO.nemo" ;
        prop:hasApp      "VIVO" ;
        prop:hasPackage  "VIVO" ;
        prop:hasTheme    "nemo" ;
        skos:prefLabel   "Selected Document"@all .

prop-data:selected_document.VIVO-languages
        rdf:type         owl:NamedIndividual ;
        rdf:type         prop:PropertyKey ;
        rdfs:label       "selected_document.VIVO-languages" ;
        prop:hasApp      "VIVO" ;
        prop:hasPackage  "VIVO-languages" ;
        skos:prefLabel   "Selected Document"@en-CA ;
        skos:prefLabel   "Selected Document"@en-US ;
        skos:prefLabel   "documento selecionado"@pt-BR ;
        skos:prefLabel   "Выбранный материал"@ru-RU ;
        skos:prefLabel   "ausgewähltes Dokument"@de-DE ;
        skos:prefLabel   "Documento seleccionado"@es ;
        skos:prefLabel   "Odabrani dokument"@sr-Latn-RS ;
        skos:prefLabel   "Document choisi"@fr-CA .
